<link rel="import" href="../polymer/polymer.html">

<dom-module id="uses-lots-of-template-ifs">
  <template>
    <template is="dom-if" if="{{x}}">
      <template is="dom-if" if="{{y}}">
        <template is="dom-if" if="{{z}}">
          <span>{{yes.hello}}</span>
        </template>
      </template>
    </template>
    <template is="dom-if" if="{{abc}}">
      <span>Good</span>
    </template>
  </template>
  <script>
    Polymer({ is: 'uses-lots-of-template-ifs' });
  </script>
</dom-module>
